<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="barbu.JeuController">
   <top>
      <HBox fx:id="topBox" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <HBox fx:id="bottomBox" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
   <left>
      <VBox fx:id="leftBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <VBox fx:id="rightBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </right>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="carteLeft" fitHeight="150.0" fitWidth="200.0" layoutX="113.0" layoutY="225.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="carteRight" fitHeight="150.0" fitWidth="200.0" layoutX="691.0" layoutY="225.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="carteTop" fitHeight="150.0" fitWidth="200.0" layoutX="400.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="carteBottom" fitHeight="150.0" fitWidth="200.0" layoutX="400.0" layoutY="406.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
